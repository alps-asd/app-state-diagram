digraph application_state_diagram {
  graph [
    labelloc="t";
    fontname="Helvetica"
    label="ALPS Blog";
    URL="index.html" target="_parent"
  ];
  node [shape = box, style = "bold,filled" fillcolor="lightgray"];

    Blog [margin=0.02, label=<<table cellspacing="0" cellpadding="5" border="0"><tr><td>Blog Post List<br />(Blog Post)<br /></td></tr></table>>,shape=box URL="#Blog" target="_parent"]
    BlogPosting [margin=0.02, label=<<table cellspacing="0" cellpadding="5" border="0"><tr><td>Blog Post<br />(identifier)<br />(create date)<br />(article body)<br /></td></tr></table>>,shape=box URL="#BlogPosting" target="_parent"]

    About -> Blog [label = <See the blog post list> URL="#goBlog" target="_parent" fontsize=13 class="goBlog" penwidth=1.5];
    Blog -> About [label = <Go to About> URL="#goAbout" target="_parent" fontsize=13 class="goAbout" penwidth=1.5];
    Blog -> Blog [label = <<b><u>Post the article</u></b>> URL="#doPost" target="_parent" fontsize=13 class="doPost" penwidth=1.5];
    Blog -> BlogPosting [label = <See the blog post> URL="#goBlogPosting" target="_parent" fontsize=13 class="goBlogPosting" penwidth=1.5];
    BlogPosting -> Blog [label = <See the blog post list> URL="#goBlog" target="_parent" fontsize=13 class="goBlog" penwidth=1.5];
    Index -> Blog [label = <See the blog post list> URL="#goBlog" target="_parent" fontsize=13 class="goBlog" penwidth=1.5];

    About [label = <About Us> URL="#About" target="_parent"]
    Index [label = <Home> URL="#Index" target="_parent"]

}