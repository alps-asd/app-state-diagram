{
  "alps": {
    "doc": {
      "value": "Simple ALPS profile for testing"
    },
    "descriptor": [
      {
        "id": "home",
        "type": "semantic",
        "doc": {
          "value": "Home state"
        },
        "descriptor": [
          {
            "href": "#goUsers"
          },
          {
            "href": "#goPosts"
          }
        ]
      },
      {
        "id": "users",
        "type": "semantic",
        "doc": {
          "value": "Users collection"
        },
        "descriptor": [
          {
            "href": "#goUser"
          }
        ]
      },
      {
        "id": "user",
        "type": "semantic",
        "doc": {
          "value": "Individual user"
        },
        "descriptor": [
          {
            "id": "name",
            "type": "semantic"
          },
          {
            "id": "email",
            "type": "semantic"
          }
        ]
      },
      {
        "id": "posts",
        "type": "semantic",
        "doc": {
          "value": "Posts collection"
        }
      },
      {
        "id": "goUsers",
        "type": "safe",
        "rt": "#users",
        "doc": {
          "value": "Go to users"
        }
      },
      {
        "id": "goPosts",
        "type": "safe",
        "rt": "#posts",
        "doc": {
          "value": "Go to posts"
        }
      },
      {
        "id": "goUser",
        "type": "safe",
        "rt": "#user",
        "doc": {
          "value": "Go to user"
        }
      }
    ]
  }
}
